{
  "name": "aws",
  "private": true,
  "version": "1.0",
  "description": "Screenshot service using the Serverless-framework",
  "main": "src/handlers.js",
  "engines": {
    "node": ">= 6.10.0"
  },
  "config": {
    "jsSrc": "src/",
    "chromiumChannel": "dev",
    "chromium_channel": "dev"
  },
  "scripts": {
    "lint": "npm run lint:eslint -s",
    "lint:eslint": "eslint $npm_package_config_jsSrc",
    "deploy": "serverless deploy -v",
    "upgrade-dependencies": "yarn upgrade-interactive --latest --exact"
  },
  "dependencies": {
    "chrome-remote-interface": "0.26.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-object-entries": "1.0.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-register": "6.26.0",
    "serverless": "1.33.2",
    "serverless-plugin-chrome": "1.0.0-55",
    "serverless-webpack": "4.0.0",
    "webpack": "3.8.1"
  },
  "babel": {
    "sourceMaps": true,
    "presets": [
      [
        "env",
        {
          "modules": "commonjs",
          "targets": {
            "node": "6.10"
          },
          "include": [
            "es7.object.values",
            "es7.object.entries"
          ]
        }
      ],
      "stage-3"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-object-entries"
    ]
  }
}
